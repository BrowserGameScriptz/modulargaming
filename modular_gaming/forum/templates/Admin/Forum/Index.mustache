<table class="table table-striped">
	<thead>
		<tr>
			<th>ID</th>
			<th>Title</th>
			<th>Description</th>
			<th>Locked</th>
			<th>Created</th>
		</tr>
	</thead>
	<tbody>
		{{#categories}}
			<tr>
				<td>{{id}}</td>
				<td>{{title}}</td>
				<td>{{description}}</td>
				<td>{{locked}}</td>
				<td>{{created}}</td>
			</tr>
		{{/categories}}
	</tbody>
</table>
